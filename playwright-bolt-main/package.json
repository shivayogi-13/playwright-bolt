{
  "name": "playwright-bolt",
  "version": "1.0.0",
  "description": "API Testing Tool with Playwright Integration",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "server": "ts-node --project src/server/tsconfig.json src/server/index.ts",
    "dev": "concurrently \"npm run start\" \"npm run server\"",
    "postinstall": "playwright install chromium"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/lab": "^5.0.0-alpha.165",
    "@mui/material": "^5.13.0",
    "ajv": "^8.12.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "framer-motion": "^10.16.4",
    "notistack": "^3.0.1",
    "otpauth": "^9.1.3",
    "otplib": "^12.0.1",
    "playwright": "^1.40.0",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "recharts": "^2.9.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^20.2.3",
    "@types/react": "^18.2.6",
    "@types/react-beautiful-dnd": "^13.1.4",
    "@types/react-dom": "^18.2.4",
    "@vitejs/plugin-react": "^4.0.0",
    "concurrently": "^8.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.5"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
